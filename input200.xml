      <zone xinitial="2" yinitial="2" xfinal="3" yfinal="3">4</zone>
   </specialcostzones>
   <obstacles num="4">
      <obstacle xpos="2" ypos="1"/>
      <obstacle xpos="2" ypos="3"/>
      <obstacle xpos="2" ypos="4"/>
      <obstacle xpos="4" ypos="2"/>
   </obstacles>
   <events>
      <death param="100"/>
      <reproduction param="1"/>
      <move param="1"/>
   </events>
</simulation>